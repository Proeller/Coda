  sub \bracketleft \bracketright by \opEmptyArray;
  sub \bar \bar by \opLogOr;
  sub \ampersand \ampersand by \opLogAnd;
  sub \less \equal by \opLessEq1;
  sub \greater \equal by \opGreaterEq1;
  sub \slash \greater by \opXmlClose;
  sub \less \slash by \opXmlEnd;
  #sub \parenleft \parenright by \opEmptyParen;
  sub \question \period by \opNullMember;
  sub \asterisk \slash by \opCommentEnd;
  #sub \quotedbl \quotedbl by \opEmptyString;
} liga;







feature clig {
  ignore sub slash slash' slash' slash';
  ignore sub slash' slash' slash' slash;
  sub slash' slash' slash' by opXmlComment;
  ignore sub slash slash' slash';
  #ignore sub colon slash' slash';
  sub slash' slash' by opComment;

  ignore sub slash' asterisk' asterisk;
  sub slash' asterisk' by \opCommentStart;
  
} clig;